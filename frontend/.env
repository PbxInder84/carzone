# Performance optimizations
GENERATE_SOURCEMAP=false
FAST_REFRESH=true
SKIP_PREFLIGHT_CHECK=true

# Increase Node memory limit (adjust based on your system)
NODE_OPTIONS=--max-old-space-size=4096

# Disable TypeScript checking during development (use separate command for this)
TSC_COMPILE_ON_ERROR=true
ESLINT_NO_DEV_ERRORS=true

# Disable opening browser automatically
BROWSER=none

# Reduce Webpack output verbosity
WDS_SOCKET_PORT=0 